{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "1.37.1",
    "blueprint": "2.1.5",
    "chokidar": "2.0.4",
    "node-sass-chokidar": "0.0.3",
    "npm": "6.1.0",
    "react": "15.6.1",
    "react-addons-css-transition-group": "15.6.0",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-scripts-ts": "2.6.0",
    "redux": "3.7.2",
    "redux-api-middleware": "2.3.0",
    "redux-thunk": "2.2.0"
  },
  "scripts": {
    "start": "npm-run-all -p build-css watch-css start-ts",
    "build": "npm-run-all -p build-css build-ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "checkout-css": "tf.exe vc checkout /recursive *.css",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-ts": "react-scripts-ts start",
    "build-ts": "react-scripts-ts build"
  },
  "devDependencies": {
    "@types/node": "10.3.1",
    "@types/prop-types": "15.5.3",
    "@types/react": "16.0.5",
    "@types/react-redux": "5.0.7",
    "@types/react-dom": "15.5.4",
    "@types/react-router-dom": "4.0.7",
    "@types/redux": "3.6.31",
    "@types/redux-thunk": "2.1.32",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "npm-run-all": "4.1.3",
    "typescript": "2.9.1"
  }
}
